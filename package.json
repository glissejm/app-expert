{
  "name": "app-expert",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://glissejm.github.io/app-expert",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "serve": "^13.0.2",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start:prod": "serve -s build",
    "start:eslint": "react-scripts start",
    "start": "craco start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js src",
    "precommit": "NODE_ENV=production lint-staged",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore .prettierrc . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.js\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15"
  }
}
